      "source": [
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "print(\"--- for文 ---\")\n",
        "for element in array:\n",
        "    print(element)\n",
        "\n",
        "print(\"--- while文 ---\")\n",
        "i = 0\n",
        "while i <= len(array) - 1:\n",
        "    print(array[i])\n",
        "    i += 1"
